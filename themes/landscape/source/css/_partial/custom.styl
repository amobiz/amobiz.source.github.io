.cheatsheet {
  overflow: hidden;
  margin: 8px 0;
  padding: 8px;
  box-shadow: 0 2px 6px 0 rgba(0,0,0,0.44);
}

.warning {
  color: red
}

.hidden {
  display: none
}

.article-entry .toc-article {
  margin-top: 2em;
}

.article-entry .toc-title {
  font-size: 1.2em;
}

.article-entry .toc, .article-entry .toc-child {
  list-style: none
}
